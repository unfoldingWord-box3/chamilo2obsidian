sed -e '
 s/O:[0-9]\+://g; 
 s/s:[0-9]\+://g;
 s/a:[0-9]\+:{}//g;
s/;a:[0-9]\+:/:\[/g;
 s/i:[0-9]\+[:;]//g;
 s/;b;"[^"]*"//g; 
  s/:[0-9]\+//g;
s/\("[a-zA-Z][^"]*"\);N;/\1:"",\n  /g;  
s/\("[a-zA-Z][^"]*"\);\("[^"]*"\);/\1: \2,\n  /g;
  
  s/{"/{\n  "/g;
  s/}}/    }\n  }/g;
  s/;}/\n  }/g;
  s/\[{/\[\n  {/g;
  s/}\[/}\n  [/g;
s/";"/",\n  "/g;
'